{
  "name": "select2-component",
  "version": "1.0.0",
  "description": "A vuejs, reactjs and angular select component.",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc -p src && tsc -p demo",
    "tslint": "tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "rev-static": "rev-static --config demo/rev-static.config.js",
    "webpack": "webpack --config demo/webpack.config.js",
    "lessc": "lessc src/select2.less > dist/select2.css",
    "stylelint": "stylelint \"src/**/*.less\"",
    "cleancss": "cleancss -o dist/select2.min.css dist/select2.css",
    "clean": "rimraf dist && rimraf demo/*.bundle-*.js",
    "file2variable": "file2variable-cli src/vue.template.html -o src/vue-variables.ts --html-minify && file2variable-cli src/angular.template.html -o src/angular-variables.ts --html-minify",
    "build": "npm run clean && npm run file2variable && npm run tsc && npm run lessc && npm run cleancss && npm run webpack && npm run rev-static",
    "lint": "npm run tslint && npm run stylelint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/select2-component.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/select2-component/issues"
  },
  "homepage": "https://github.com/plantain-00/select2-component#readme",
  "devDependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@types/react": "15.0.25",
    "@types/react-dom": "15.5.0",
    "clean-css-cli": "4.1.3",
    "core-js": "2.4.1",
    "file2variable-cli": "1.3.0",
    "less": "2.7.2",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "rev-static": "2.4.2",
    "rimraf": "2.6.1",
    "rxjs": "5.4.0",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "tslint": "5.4.0",
    "typescript": "2.3.4",
    "vue": "2.3.3",
    "vue-class-component": "5.0.1",
    "webpack": "2.6.1",
    "zone.js": "0.8.11"
  },
  "dependencies": {
    "tslib": "1.7.1"
  }
}
